		      <mass value="0.2"/>
		      <origin rpy="${1.570796*orientation} 0 0" xyz="${-0.3865*end} ${-(0.15516+0.134 + 0.078)*orientation} -0.22012"/>
		      <inertia ixx="0.000526666666667" ixy="0" ixz="0" iyy="0.000526666666667" iyz="0" izz="0.001"/>
		    </inertial>
		    <collision name="${name}_collision">
