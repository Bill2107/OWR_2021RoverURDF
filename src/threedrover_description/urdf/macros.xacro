<?xml version="1.0" ?>
<robot xmlns:xacro="https://www.ros.org/wiki/xacro">
<!-- LINK_BOGIE ********************************************************** -->
	<macro name ="link_bogie_stat" params="name orientation">
		<link name="${name}">
			<!-- values from the fusion360 files -->
		    <inertial>
		      <mass value="0.2"/>
		      <origin rpy="${1.570796*orientation} 0 0" xyz="0 ${-0.15516*orientation} 0"/>
		      <inertia ixx="0.000526666666667" ixy="0" ixz="0" iyy="0.000526666666667" iyz="0" izz="0.001"/>
		    </inertial>
		    <collision name="${name}_collision">
		      <origin rpy="${1.570796*orientation} 0 0" xyz="0 ${-0.15516*orientation} 0"/>
	      		<geometry>
				<mesh filename="package://threedrover_description/MODELS/OWR_20_BOGIE_00_ASM_STAT.stl" scale="0.001 0.001 0.001" />
  		        </geometry>
		    </collision>
		    <visual name="${name}_visual">
		      <origin rpy="${1.570796*orientation} 0 0" xyz="0 ${-0.15516*orientation} 0"/>
		      	<geometry>
				<mesh filename="package://threedrover_description/MODELS/OWR_20_BOGIE_00_ASM_STAT.stl" scale="0.001 0.001 0.001" />
		     	 </geometry>
		    </visual>
	   	</link>
    	</macro>
	<macro name ="link_bogie_rot" params="name orientation">
		<link name="${name}">
			<!-- values from the fusion360 files -->
		    <inertial>
		      <mass value="0.2"/>
		      <origin rpy="${1.570796*orientation} ${(orientation ==-1)*(3.14) +orientation*0.06925171} 0" xyz="0 ${-0.15516*orientation} 0"/>
		      <inertia ixx="0.000526666666667" ixy="0" ixz="0" iyy="0.000526666666667" iyz="0" izz="0.001"/>
		    </inertial>
		    <collision name="${name}_collision">
		      <origin rpy="${1.570796*orientation} ${(orientation ==-1)*3.14 +orientation*0.06925171} 0" xyz="0 ${-0.15516*orientation} 0"/>
	      		<geometry>
				<mesh filename="package://threedrover_description/MODELS/OWR_20_BOGIE_00_ASM_ROT.stl" scale="0.001 0.001 0.001" />
  		        </geometry>
		    </collision>
		    <visual name="${name}_visual">
		      <origin rpy="${1.570796*orientation} ${(orientation ==-1)*3.14 +orientation*0.06925171} 0" xyz="0 ${-0.15516*orientation} 0"/>
		      	<geometry>
				<mesh filename="package://threedrover_description/MODELS/OWR_20_BOGIE_00_ASM_ROT.stl" scale="0.001 0.001 0.001" />
		     	 </geometry>
		    </visual>
	   	</link>
    	</macro>

<!-- LINK_WHEEL ********************************************************** -->
	<macro name ="link_wheel_stat" params="name orientation end">
